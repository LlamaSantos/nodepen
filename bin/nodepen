#!/usr/bin/env node

var path = require('path');
var cp = require('child_process');
var exec = cp.exec;

var launchDir = path.join(process.cwd());
var cmd = './node_modules/nodewebkit/bin/nodewebkit . ' + launchDir;

var child = exec(cmd, {cwd: path.resolve(__dirname, "..") });
child.unref();
process.exit();
